#include<stdio.h>
int main()
  {
  int n, i, k;
  k=1;
   printf("Enter a number:");
   scanf("%d",&n);
   for(i=2;i<=n;i++)
  {
   if(n%i==0)
   k=k+1;
 N  }

  if(n==1)
  printf("\n1 is not a prime number nor a camposite number.");
   else
 {
     if(k>2)
     printf("\n%d is  a camposite number ",n);

    else 
    printf("\n%d is  a prime number ",n);
  }
return 0;
}

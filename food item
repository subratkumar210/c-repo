#include<stdio.h>
int main()
{
 int n;
 printf("Enter any number between 1 to 5\n");
 scanf("%d",&n);
switch (n)
{
   case(1):
   {
    printf("Food item-Pizza \n");
    printf("Price - Rs 229\n");
   }

 case(2):
   {
    printf("Food item- Burger \n");
    printf("Price - Rs 129\n");
   }
 break;

 case(3):
   {
    printf("Food item-Pasta \n");
    printf("Price - Rs179\n");
   }
break ;

 case(4):
   {
    printf("Food item- French Fries \n");
    printf("Price - Rs 99\n");
   }
 break ;
 case(5):
   {
    printf("Food item- Sanwich\n");
    printf("Price - Rs149\n");
   }
 break ;
default:
 printf("Enter valid number ");
 
}
return 0;

}

#include <stdio.h>
void main()
{
   int mat[3][3];
   int i, j, sum =0 ;
   for(i=0;i<=2;i++)
   {
     for(j=0;j<=2;j++)
      {
        printf("\nEnter (%d,%d) element of metrix:",(i+1),(j+1));
        scanf("%d",& mat[i][j]);   
      }
   }
  
   for(i=0;i<=2;i++)
{
   sum =sum +mat [i][i];
  }
   for(i=0;i<=2;i++)
   {
     for(j=0;j<=2;j++)
      
        {printf("%d",mat[i][j]);
         printf(" ");
         }
        printf("\n");
   }
   printf("\nsum of diagonal elements of above matrix:%d",sum);

}
